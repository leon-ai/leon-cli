{
  "extends": ["conventions", "prettier"],
  "plugins": ["prettier", "import", "unicorn"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/extensions": ["error", "always"],
    "unicorn/prefer-node-protocol": "error"
  }
}
